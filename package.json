{
  "name": "interval-operations",
  "version": "1.0.4",
  "description": "Utilities for performing mathematical set operations on intervals and arrays of intervals",
  "keywords": [
    "interval",
    "time interval",
    "interval utils",
    "set operations",
    "union",
    "intersection",
    "difference",
    "mathematics"
  ],
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "rollup --config"
  },
  "author": "Andreas Anckar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aanckar/interval-operations.git"
  },
  "homepage": "https://github.com/aanckar/interval-operations",
  "bugs": {
    "url": "https://github.com/aanckar/interval-operations/issues"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rollup": "^1.31.1",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
