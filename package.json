{
  "name": "interval-operations",
  "version": "1.0.7",
  "description": "Utilities for performing mathematical set operations on intervals and arrays of intervals",
  "keywords": [
    "interval",
    "time interval",
    "interval utils",
    "set operations",
    "union",
    "intersection",
    "difference",
    "mathematics"
  ],
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "rollup --config"
  },
  "author": "Andreas Anckar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aanckar/interval-operations.git"
  },
  "homepage": "https://github.com/aanckar/interval-operations",
  "bugs": {
    "url": "https://github.com/aanckar/interval-operations/issues"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.32",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.1.8",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup": "^2.40.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
